#include <iostream>

using namespace std;

int main()
{
	cout << "\nhello, world";     //string literal -- insertion operation
	
	cout << endl;     //advance down one line -- creates blank line 	
	
	cout << "\ngoodbye for now..";
	
	return 0;
}
